---

env:
  proxies:
    http_proxy:
    https_proxy:

macports_packages:
  - erlang
  - htop
  - "'MacVim @7.4.snapshot73_1+huge+python27+ruby20'"
  - php
  - python27
  - readline
  - the_silver_searcher
  - tmux
  - tor

dotfiles:
  dir: ~/.dotfiles
  git:
    git_path: /usr/bin/git
  rcm:
    rcup_path: /opt/local/bin/rcup
    repo:
      name: https://github.com/ryankanno/dotfiles.git
      version: HEAD

dock:
  persistent_apps:
    - /Applications/iTerm.app
